var number = 1
while number <= 100 {
    let fizz = number % 3 == 0
    let buzz = number % 5 == 0
    if fizz && buzz {
        say("FizzBuzz")
    } else {
        if fizz {
            say("Fizz")
        } else {
            if buzz {
                say("Buzz")
            } else {
                write(number)
            }
        }
    }
    number = number + 1
}
