var number = 1
while number <= 100 {
    let fizz = "Fizz"
    let buzz = "Buzz"
    let fizzBuzz = "FizzBuzz"
    let isFizz = number % 3 == 0
    let isBuzz = number % 5 == 0
    if isFizz && isBuzz {
        say(fizzBuzz)
    } else {
        if isFizz {
            say(fizz)
        } else {
            if isBuzz {
                say(buzz)
            } else {
                write(number)
            }
        }
    }
    number = number + 1
}
