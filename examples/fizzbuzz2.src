var number = 1;
while number <= 100 {
    if number % 3 == 0 && number % 5 == 0 {
        say("FizzBuzz");
    } else {
        if number % 3 == 0 {
            say("Fizz");
        } else {
            if number % 5 == 0 {
                say("Buzz");
            } else {
                write(number);
            }
        }
    }
    number = number + 1;
}
